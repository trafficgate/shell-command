{
    "name": "trafficgate/shell-command",
    "description": "Interface for creating PHP objects that wrap shell commands",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/trafficgate/shell-command/issues",
        "source": "https://github.com/trafficgate/shell-command"
    },
    "authors": [
        {
            "name": "Andrew Miller",
            "email": "andrew.miller@rakuten.com"
        }
    ],
    "require": {
        "php": "^7.0.0",
        "symfony/process": "~3.0|~4.0",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "friendsofphp/php-cs-fixer": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Trafficgate\\Shell\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Trafficgate\\Shell\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable"
}
